package modele;

public class DeveloppeurDesktop extends Developpeur{
	
	private boolean certifié_LINUX;

	public DeveloppeurDesktop() {
		super();
		
	}

	public DeveloppeurDesktop(String nom, String prenom, String date_embauche, String ancienneté) {
		super(nom, prenom, date_embauche, ancienneté);
		
	}

	public DeveloppeurDesktop(boolean certifié_LINUX) {
		super();
		this.certifié_LINUX = certifié_LINUX;
	}
	

}
